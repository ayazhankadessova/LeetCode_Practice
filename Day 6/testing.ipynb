    "word = 'hello'\n",
    "\n",
    "word2 = 'hi'\n",
    "\n",
    "\n",
    "def mask(word):\n",
    "    mask_a = 0\n",
    "    for c in word:\n",
    "        mask_a |= (1 << (ord(c) - ord('a')))\n",
    "    return mask_a\n",
    "\n",
    "if mask(word) & mask(word2)> 0:\n",
    "    print('two strings have common characters')"
   ]
  }
